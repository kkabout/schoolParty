<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.schoolParty.dao.MessageDao">
    <select id="addMessage">
        INSERT INTO message (fromId,fromName,toId,messageText,messageDate)
        values (
        #{formId},#{fromName},#{toId},#{messageText},#{messageDate}
        );
    </select>
</mapper>